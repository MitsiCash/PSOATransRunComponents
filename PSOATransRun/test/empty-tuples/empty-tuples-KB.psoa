RuleML (
  Assert (
    o#p()
    o#p(+[])
    o#p(-[])
    o#p(a)
    o#p(+[a])
    o#p(-[a])
    o#p(a b)
    o#p(+[a b])
    o#p(-[a b])
    o#p(a b c)
    o#p(+[a b c])
    o#p(-[a b c])
    o#p(+[] +[a] +[a b] +[a b c] -[] -[a] -[a b] -[a b c])
    Forall ?o (
      ?o#q(+[]) :-
        And(?o#p(-[])
            o#p(+[] +[a] +[a b] +[a b c] -[] -[a] -[a b] -[a b c])
        )
    )
  )
)